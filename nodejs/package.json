{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "mov": "nodemon src/index.js",

    "migrate": " cd src && npx sequelize-cli db:migrate",
    "migrate:create": "cd src && npx sequelize-cli model:generate --name eventsToUsers --attributes userId:integer,eventId:integer",
    "migrate:undo": "cd src && npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "cd src && npx sequelize-cli db:migrate:undo:all",

    "seed": "cd src && npx sequelize-cli db:seed:all",
    "seed:create": "cd src && npx sequelize-cli seed:generate --name demo-user",
    "seed:undo": "cd src && npx sequelize-cli db:seed:undo",
    "seed:undo:all": "cd src && npx sequelize-cli db:seed:undo:all",

    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.28.2",
    "body-parser": "^1.19.0",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.6.5",
    "nodemon": "^2.0.13",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
